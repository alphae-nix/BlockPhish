{
  "manifest_version": 3,
  "name": "phishdetector",
  "version": "0.1",

  "background": {
    "service_worker": "background.js"
  },
  
   
  "permissions": ["storage", "activeTab", "scripting","notifications"],
    
  "action": {
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    },
    "default_popup": "popup.html"

  },
    
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  }

}